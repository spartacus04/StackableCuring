{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "villagerBlacklist": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "ARMORER",
          "BUTCHER",
          "CARTOGRAPHER",
          "CLERIC",
          "FARMER",
          "FISHERMAN",
          "FLETCHER",
          "LEATHERWORKER",
          "LIBRARIAN",
          "MASON",
          "SHEPHERD",
          "TOOLSMITH",
          "WEAPONSMITH",
          "NONE",
          "NITWIT"
        ]
      }
    },
    "uninstallMode": {
      "type": "boolean"
    },
    "updateCheck": {
      "type": "boolean"
    }
  },
  "required": [
    "villagerBlacklist",
    "uninstallMode",
    "updateCheck"
  ]
}